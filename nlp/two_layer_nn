// Two Layer Neural Network
digraph {
	I1 [label="Input 1"]
	I2 [label="Input 2"]
	I3 [label="Input 3"]
	H1 [label="Hidden 1"]
	H2 [label="Hidden 2"]
	H3 [label="Hidden 3"]
	H4 [label="Hidden 4"]
	O1 [label="Output 1"]
	I1 -> H1
	I1 -> H2
	I1 -> H3
	I1 -> H4
	I2 -> H1
	I2 -> H2
	I2 -> H3
	I2 -> H4
	I3 -> H1
	I3 -> H2
	I3 -> H3
	I3 -> H4
	H1 -> O1
	H2 -> O1
	H3 -> O1
	H4 -> O1
}
